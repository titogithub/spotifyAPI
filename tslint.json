{
    "defaultSeverity": "error",
    "extends": ["tslint:latest", "tslint-config-prettier"],
    "jsRules": {
        "max-line-length": {
            "options": [120]
        }
    },
    "rules": {
        "member-access": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "interface-name": false,
        "no-console": false,
        "max-line-length": {
            "options": [120]
        }
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": ["dist/", "node_modules"]
    }
}
